#include<iostream>
#include<string> 
using namespace std;

int pow(int b,int p){
	
	if(p==1){
		return b;
	}else{
		return b * pow(b,p-1);
	}
	
}

int main(){
	
	ios::sync_with_stdio(0);
	cin.tie(0);
	
	long long int sum;
	int b,p,m;
	
	while(cin >> b >> p >> m){
		
		sum = pow(b, p);
		sum %= m;
		cout << sum << "\n";
		
	}
	
	return 0;
	
}
