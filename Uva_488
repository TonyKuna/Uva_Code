#include<iostream>
using namespace std;
int main()
{
	int n,a,b,flag=1;
	cin >> n;
	while(n>0){
		
		cin >> a >> b;
		for(int i=0;i<b;i++){
			
			if(flag==0){
				cout << "\n";
			}
			
			for(int k=0;k<a;k++){
				for(int j=0;j<=k;j++){
					cout << k+1;
				}
				cout << "\n";
			}
			
			for(int k=a-1;k>0;k--){
				for(int j=0;j<k;j++){
					cout << k;
				}
				cout << "\n";
			}
			
			flag=0;
			
		}
		n--;
	}


	return 0;
}
